<?xml version="1.0" encoding="UTF-8"?>
<faces-config
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
        version="2.0">
    <managed-bean>
        <managed-bean-name>partTypeDao</managed-bean-name>
        <managed-bean-class>com.aliyektan.project.core.dao.PartTypeDao</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>customerDao</managed-bean-name>
        <managed-bean-class>com.aliyektan.project.core.dao.CustomerDao</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>partDao</managed-bean-name>
        <managed-bean-class>com.aliyektan.project.core.dao.PartDao</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>processDao</managed-bean-name>
        <managed-bean-class>com.aliyektan.project.core.dao.ProcessDao</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>processTypeDao</managed-bean-name>
        <managed-bean-class>com.aliyektan.project.core.dao.ProcessTypeDao</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>productDao</managed-bean-name>
        <managed-bean-class>com.aliyektan.project.core.dao.ProductDao</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>productTypeDao</managed-bean-name>
        <managed-bean-class>com.aliyektan.project.core.dao.ProductTypeDao</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>userDao</managed-bean-name>
        <managed-bean-class>com.aliyektan.project.core.account.dao.UserDao</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>roleDao</managed-bean-name>
        <managed-bean-class>com.aliyektan.project.core.account.dao.RoleDao</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>



    <navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>